# data.frames
listaAlunos$nome # retorna apenas a coluna nome do data.frame listaAalunos

listaAlunos[ , 2] # retorna todas as linhas apenas a coluna data de nascimeto do data.frame iris

listaAlunos[1:3, 2:3] # retorna as 3 primeiras linhas das colunas 2 a 5 do data.frame listaAlunos

listaAlunos[, 'nome'] # retorna a coluna nome, indexada pelo nome
